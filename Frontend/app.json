        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "94a19c48-682a-47ca-94bc-2084a8ec443d"
      }
    },
    "owner": "subhadeepbhattacharyya",
    "ios": {
      "bundleIdentifier": "com.clickbasket.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    }
  }
